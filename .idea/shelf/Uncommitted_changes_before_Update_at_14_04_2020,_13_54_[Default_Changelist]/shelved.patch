<+><!DOCTYPE html>\n\n<html lang=\"en\">\n<%- include('layouts/header.ejs');-%>\n\n<body>\n<%- include('layouts/navbar.ejs');-%>\n<%- include('layouts/scripts');-%>\n\n<script>\n    function onload(){\n        console.log(\"test\")\n    }\n    onload();\n</script>\n\n<h1>\n    Admin can register a linestander and update/delete linestander\n</h1>\n\n<form action=\"/users/registerLinestander\" method=\"POST\" enctype=\"application/x-www-form-urlencoded\">\n    <div class=\"container\">\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <label for=\"fullname\"><b>Linestander full name</b></label><br>\n                <input type=\"text\" placeholder=\"Full name\" id=\"fullName\" name=\"fullName\">\n            </div>\n        </div>\n        <br>\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <label for=\"email\"><b>Email</b></label><br>\n                <input type=\"text\" placeholder=\"Email\" id=\"email\" name=\"email\">\n            </div>\n        </div>\n        <br>\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <label for=\"phoneNumber\"><b>Phone number</b></label><br>\n                <input type=\"text\" placeholder=\"Phone number\" id=\"phoneNumber\" name=\"phoneNumber\">\n            </div>\n        </div>\n        <br>\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <label for=\"Username\"><b>Linestander name</b></label><br>\n                <input type=\"text\" placeholder=\"Username\" id=\"username\" name=\"username\">\n            </div>\n        </div>\n        <br>\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group control\">\n                <label for=\"psw\"><b>Linestander password</b></label><br>\n                <input type=\"password\" placeholder=\"Enter Password\" id=\"password\" name=\"password\">\n            </div>\n        </div>\n        <br>\n        <button type=\"submit\" class=\"registerbtn\">Register</button>\n        <hr>\n\n    </div>\n</form>\n\n<section class=\"get\">\n    <h3> GET data</h3>\n    <div>\n        <% for(var i = 0; i < users.length; i++) { %>\n       <article class=\"data\">\n           <hr>\n           <div> <b>Username:</b> <%= users[i].username %> </div>\n           <br>\n           <div> <b>Full name:</b> <%= users[i].lineStander.fullName %> </div>\n           <br>\n           <div> <b>Email:</b> <%= users[i].lineStander.email %> </div>\n           <br>\n           <div> <b>phoneNumber:</b> <%= users[i].lineStander.phoneNumber %> </div>\n           <br>\n           <div> <b>ID:</b>  <%= users[i]._id %></div>\n\n       </article>\n        <% } %>\n    </div>\n</section>\n\n\n\n<section class=\"update\">\n    <h3>UPDATE data</h3>\n    <form id=\"updateForm\" name=\"updateForm\" action=\"/update\">\n        <div class=\"input\">\n            <label for=\"id\"> ID </label>\n            <input type=\"text\" id=\"id\" name=\"id\">\n        </div>\n        <br>\n        <div class=\"input\">\n            <label for=\"username\"> Username </label>\n            <input type=\"text\" id=\"username\" name=\"username\">\n        </div>\n        <br>\n        <button type=\"submit\" id=\"formButton\" name=\"formButton\" >Update</button>\n    </form>\n</section>\n\n<section class=\"new_product\">\n    <h3>ADD A NEW PRODUCT</h3>\n    <form action=\"/product/new\" method =\"POST\" enctype=\"application/x-www-form-urlencoded\">\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <div class=\"input\">\n                    <label>New product</label>\n                    <input type=\"text\" class=\"form-control\" placeholder=\"Product name\" id=\"productName\" name=\"productName\">\n                </div>\n            </div>\n            <br>\n        </div>\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <div class=\"input\">\n                    <label>Price</label>\n                    <input type=\"number\" class=\"form-control\" placeholder=\"Product price\" id=\"price\" name=\"price\">\n                </div>\n            </div>\n        </div>\n        <br>\n        <div class=\"form-group\">\n            <button type=\"submit\" class=\"btn btn-primary\" id=\"addProduct\">Add order</button>\n        </div>\n    </form>\n</section>\n\n<section class=\"delete\">\n    <h3>DELETE</h3>\n    <form action=\"/delete\" method =\"POST\" enctype=\"application/x-www-form-urlencoded\">\n        <div class=\"control-group\">\n            <div class=\"form-group floating-label-form-group controls\">\n                <div class=\"input\">\n                    <label>ID</label>\n                    <input type=\"text\" class=\"form-control\" placeholder=\"ID\" id=\"id\" name=\"id\">\n                </div>\n            </div>\n            <div></div>\n            <br>\n        </div>\n        <button type=\"submit\"> Delete</button>\n\n    </form>\n</section>\n\n<script src=\"/public/js/updateUser.js\"></script>\n\n\n</body>\n\n\n</html>\n
